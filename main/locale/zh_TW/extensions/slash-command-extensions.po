msgid "\"{invalid_option} is not a valid option\""
msgstr ""

#: src/extensions/slash-commands.md:78
msgid "\"You chose {selection}.\""
msgstr ""

#: src/extensions/slash-commands.md:83
msgid "\"Pick One: {selection}\""
msgstr ""

