#: src/configuring-zed.md:4260
msgid "\"[)}\\\\]>]?\""
msgstr ""

#: src/configuring-zed.md:4261
msgid "\"# if line/column matched, may be followed by a description\""
msgstr ""

#: src/configuring-zed.md:4262
msgid "\"(?(<line_column>):[^ 0-9][^ ]*)?\""
msgstr ""

#: src/configuring-zed.md:4263
msgid "\"# which may be followed by trailing punctuation\""
