msgstr ""

#: src/linux.md:348
msgid "`96` for standard 1x scaling"
msgstr ""

#: src/linux.md:349
msgid "`144` for 1.5x scaling"
msgstr ""

#: src/linux.md:350
msgid "`192` for 2x scaling"
msgstr ""

#: src/linux.md:351
msgid "`288` for 3x scaling"
msgstr ""

#: src/linux.md:353
msgid "Load the configuration:"
msgstr ""

#: src/linux.md:359
